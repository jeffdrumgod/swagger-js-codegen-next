/**
 * {{&summary}}
 * @method
 * @name {{&className}}#{{&methodName}}
{{#parameters}}
{{^isSingleton}} * @param {{=<% %>=}}{<%&type%>}<%={{ }}=%> {{&camelCaseName}} - {{&description}}{{/isSingleton}}
{{/parameters}}
 *
 */
{{&methodName}}({{#hasParams}}parameters = {}{{/hasParams}}) {
	{{#hasFormParameter}}const form = {};{{/hasFormParameter}}
	{{#hasBodyParameter}}let data = {};{{/hasBodyParameter}}
	let path = '{{&domain}}{{&basePath}}{{&path}}';
	{{#hasQueryParameter}}const params = {};{{/hasQueryParameter}}
	{{#hasHeaderParameter}}const headers = {};{{/hasHeaderParameter}}

	{{#headers}}
		headers['{{&name}}'] = [{{&value}}];
	{{/headers}}

	{{#parameters}}
	{{#allowGetExternal}}parameters['{{&camelCaseName}}'] = parameters['{{&camelCaseName}}'] || this.getExternal('{{&camelCaseName}}');{{/allowGetExternal}}
	{{#required}}
	if(parameters['{{&camelCaseName}}'] === undefined){
		return this.rejectMissingRequired('{{&type}}', '{{&camelCaseName}}');
	}
	{{/required}}

	{{#isQueryParameter}}
	if(parameters['{{&camelCaseName}}'] !== undefined){
		params['{{&name}}'] = parameters['{{&camelCaseName}}'];
		
		if ('{{&type}}' === 'array') {
			params['{{&name}}'] = params['{{&name}}'].join(',');
		}
	}
	{{/isQueryParameter}}

	{{#isBodyParameter}}
	if(parameters['{{&camelCaseName}}'] !== undefined){
		data = parameters['{{&camelCaseName}}'];
	}
	{{/isBodyParameter}}

	{{#isPathParameter}}
	if(parameters['{{&camelCaseName}}'] !== undefined){
		path = path.replace('{{=<% %>=}}{<%& camelCaseName %>}<%={{ }}=%>', parameters['{{&camelCaseName}}']);
	}
	{{/isPathParameter}}

	{{#isHeaderParameter}}
	headers['{{&name}}'] = parameters['{{&camelCaseName}}'];
	{{/isHeaderParameter}}

	{{#isFormParameter}}
	form['{{&name}}'] = parameters['{{&camelCaseName}}'];
	{{/isFormParameter}}
	{{/parameters}}

	return this.request({
		method: '{{method}}',
		url: path,
		{{#hasQueryParameter}}params,{{/hasQueryParameter}}
		{{#hasBodyParameter}}data,{{/hasBodyParameter}}
		{{#hasHeaderParameter}}headers,{{/hasHeaderParameter}}
	});
}
