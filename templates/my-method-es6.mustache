/**
 * {{&summary}}
 * @method
 * @name {{&className}}#{{&methodName}}
{{#parameters}}
{{^isSingleton}} * @param {{=<% %>=}}{<%&type%>}<%={{ }}=%> {{&camelCaseName}} - {{&description}}{{/isSingleton}}
{{/parameters}}
 *
 */
{{&methodName}}(parameters = {}) {
	const form = {};
	{{#isBodyParameter}}let body;{{/isBodyParameter}}
	{{#headers}}const headers = {};{{/headers}}

	{{#headers}}
		headers['{{&name}}'] = [{{&value}}];
	{{/headers}}

	parameters['headers'] = headers;

	{{#isBodyParameter}}
	if(parameters['{{&camelCaseName}}'] !== undefined){
		body = parameters['{{&camelCaseName}}'];
	}
	{{/isBodyParameter}}

	{{#parameters}}
	{{#allowGetExternal}}parameters['{{&camelCaseName}}'] = parameters['{{&camelCaseName}}'] || this.getExternal('{{&camelCaseName}}');{{/allowGetExternal}}
	{{#required}}
	if(parameters['{{&camelCaseName}}'] === undefined){
		return this.rejectMissingRequired('{{&paramType}}', '{{&camelCaseName}}');
	}
	{{/required}}
	form['{{&name}}'] = parameters['{{&camelCaseName}}'];
	{{/parameters}}

	return this.request({
		method: '{{method}}',
		url: '{{&path}}',
		params: form,
		{{#headers}}headers,{{/headers}}
		{{#isBodyParameter}}data: body,{{/isBodyParameter}}
	});
}
